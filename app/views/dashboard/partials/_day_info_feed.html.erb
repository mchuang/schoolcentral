<div id="day-info-feed-panel" class="col-sm-3">
	<div class="panel panel-default">
	  <div class="panel-heading">
	    <h3 class="panel-title center-text"><%= @d.strftime("%a, %d %b %Y") %></h3>
	  </div>
	  <div class="panel-body">
	    <% for event in @events%>
	    	<% if event.owner_type == "Assignment"%>
	    		<div>
	    			<%= event.startime.strftime("%I:%M%p")%>:<%= link_to event.owner.name, event.owner %>
	    		</div>
	    	<% end %>
	    <% end %>
	  </div>
	</div>
</div>