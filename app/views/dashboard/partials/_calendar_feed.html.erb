<div class="col-sm-7">
	<div id="calendar" class="container-fluid calendar">
		<div id="calendar-header" class="row calendar-header">
			<h2 class="month-label">Month</h2>
			<div class="btn-group calendar-navigation">
				<button type="button" class="btn btn-default" onclick="previousMonth()">prev</button>
				<button type="button" class="btn btn-default" onclick="setCalendarToToday()">today</button>
				<button type="button" class="btn btn-default" onclick="nextMonth()">next</button>
			</div>
		</div>
		<div class="row calendar-week-header">
			<div class="calendar-day day-label">sun</div>
	        <div class="calendar-day day-label">mon</div>
	      	<div class="calendar-day day-label">tue</div>
	      	<div class="calendar-day day-label">wed</div>
	      	<div class="calendar-day day-label">thu</div>
	      	<div class="calendar-day day-label">fri</div>
	      	<div class="calendar-day day-label">sat</div>
	    </div>
		
		<div id="calendar-content">
		</div>

	</div>
</div>
<script>
	var selectedMonth = new Date();
	var dateSelected = false;
	$(document).ready(function() {
    	setCalendarToToday();
	});
</script>